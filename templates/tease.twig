<article class="tease tease-{{post.post_type}}" id="tease-{{post.ID}}">
	{% block content %}
		{% include "partial/card.twig" with {
            title: post.title,
            link: post.link,
            content: post.preview.length(25), 
            thumbnail: post.thumbnail.src,
            tags: post.tags
        } %}
	{% endblock %}
</article>